{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node",
      "@nuxt/types",
      "@pinia/nuxt",
      "@nuxtjs/supabase"
    ],
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#imports": ["./.nuxt/types/imports"],
      "#app": ["./.nuxt/types/app"]
    },
    "noImplicitAny": false
  },
  "exclude": ["node_modules"]
}
